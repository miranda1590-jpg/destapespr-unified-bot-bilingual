{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "off"
  }
  ,
  "overrides": [
    {
      "files": ["**/__tests__/**", "**/*.test.js"],
      "env": { "jest": true }
    }
  ]
}
